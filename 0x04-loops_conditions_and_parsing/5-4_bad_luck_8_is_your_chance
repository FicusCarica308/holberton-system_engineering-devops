#!/usr/bin/env bash
#prints "Holberton School" for every iteration except 4 where "bad luck" is and 8 where "good luck" is
iter=1
while [ $iter -le 10 ]; do
    if [ $iter == 4 ]; then
        echo "bad luck"
    elif [ $iter == 8 ]; then
        echo "good luck"
    else
        echo "Holberton School"
    fi
    iter=$((iter+1))
done
